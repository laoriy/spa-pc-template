{
    "editor.fontSize": 16,
    "editor.tabSize": 4,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "vue",
        "html",
        "javascript",
        "typescript",
        "javascriptreact",
        "typescriptreact"
    ],
    "pathAlias.aliasMap": {
        "@": "${cwd}/src",
        "model": "${cwd}/src/views/screen/model",
        "utils": "${cwd}/src/views/screen/utils",
    },
    "cSpell.words": [
        "cmpt"
    ]
}